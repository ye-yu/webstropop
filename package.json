{
  "name": "webstropop",
  "version": "1.0.0",
  "description": "An AstroPop parody web game",
  "main": "./dist/webstropop.js",
  "scripts": {
    "dev": "npm run build && npm run watch",
    "build": "tsc",
    "watch": "gulp -f gulp.dist.js",
    "vendor": "gulp -f gulp.dist.js vendor",
    "clean": "rimraf ./dist/* && rimraf ./tsbuild/*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ye-yu/webstropop.git"
  },
  "keywords": [
    "phaserjs"
  ],
  "author": "Ye-Yu",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ye-yu/webstropop/issues"
  },
  "homepage": "https://github.com/ye-yu/webstropop#readme",
  "dependencies": {
    "phaser": "^3.53.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.34",
    "browserify": "^17.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3",
    "vinyl-source-stream": "^2.0.0"
  }
}
